
public class Binary_Encoder {
	private char continut;
	Binary_Encoder(char _continut)
	{
		this.continut = _continut;
	}
	public void setContinut(char _continut)
	{
		this.continut = _continut;
	}
	public char getContinut()
	{
		return this.continut;
	}
	 public void encoder()
	    {
		 /*
	    	Random rand = new Random();
	        int n1 = rand.nextInt(100);;
	        int n2 = 255 + n1;
	        this.continut = (char) (n1 + (continut+n1	) % (n2-n1));
	        */
		 this.continut = (char)(this.continut +1);
	    }
}
